{
  "name": "jsdelivr",
  "version": 2,
  "alias": "jsdelivr",
  "regions": ["hnd", "sfo"],
  "builds": [
    { "src": "package.json", "use": "@now/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    {"src": "^/js/(.*)", "dest": "/js/$1"},
    {"src": "^/css/(.*)", "dest": "/css/$1"},
    {"src": "^/img/(.*)", "dest": "/img/$1"},
    {"src": "^/favicon.ico", "dest": "/favicon.ico"},
    {"src": ".*", "dest": "/index.html"}
  ]
}
